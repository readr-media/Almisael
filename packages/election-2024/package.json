{
  "name": "election-2024",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "build:dev-mirrorMedia": "NEXT_PUBLIC_RELEASE_TARGET='dev-mirrorMedia' next build",
    "build:mirrorMedia": "NEXT_PUBLIC_RELEASE_TARGET='mirrorMedia' next build",
    "build:dev-mirrorTV": "NEXT_PUBLIC_RELEASE_TARGET='dev-mirrorTV' next build",
    "build:mirrorTV": "NEXT_PUBLIC_RELEASE_TARGET='mirrorTV' next build",
    "build:readr": "NEXT_PUBLIC_RELEASE_TARGET='readr' next build",
    "export:dev-mirrorMedia": "yarn build:dev-mirrorMedia && next export -o out/dev-mirrorMedia",
    "export:mirrorMedia": "yarn build:mirrorMedia && next export -o out/mirrorMedia",
    "export:dev-mirrorTV": "yarn build:dev-mirrorTV && next export -o out/dev-mirrorTV",
    "export:mirrorTV": "yarn build:mirrorTV && next export -o out/mirrorTV",
    "export:readr": "yarn build:readr && next export -o out/readr",
    "export:all": "yarn export:dev-mirrorMedia && yarn export:mirrorMedia && yarn export:dev-mirrorTV && yarn export:mirrorTV && yarn export:readr",
    "deploy:dev-mirrorMedia": "NEXT_PUBLIC_RELEASE_TARGET='dev-mirrorMedia' node ./deploy-scripts/index.mjs",
    "deploy:mirrorMedia": "NEXT_PUBLIC_RELEASE_TARGET='mirrorMedia' node ./deploy-scripts/index.mjs",
    "deploy:dev-mirrorTV": "NEXT_PUBLIC_RELEASE_TARGET='dev-mirrorTV' node ./deploy-scripts/index.mjs",
    "deploy:mirrorTV": "NEXT_PUBLIC_RELEASE_TARGET='mirrorTV' node ./deploy-scripts/index.mjs",
    "deploy:readr": "NEXT_PUBLIC_RELEASE_TARGET='readr' node ./deploy-scripts/index.mjs"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "^3.39.3",
    "styled-components": "^6.1.2",
    "swr": "^2.2.4"
  }
}
